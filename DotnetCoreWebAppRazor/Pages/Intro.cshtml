@page
@model DotnetCoreWebAppRazor.Pages.IntroModel
@{
    ViewData["Title"] = "Intro Page";
}
<body>
    <h3>Satya Venkata Rohit Godavarthi</h3>
    <form method="post">
        <fieldset>
            <legend>Enter numbers</legend>
            <table>
                <tr>
                    <td><label for="FirstNumber">First Number:</label></td>
                    <td><input type="number" name="FirstNumber" value="" /></td>
                </tr>
                <tr>
                    <td><label for="SecondNumber">Second Number:</label></td>
                    <td><input type="number" name="SecondNumber" value="" /></td>
                </tr>
                <tr>
                    <td><label for="Output">Output:</label></td>
                    <td><label for="Output">@Model.Output</label></td>
                </tr>
                <tr>
                    <td><input type="submit" value="Submit" asp-page-handler="Submit" /></td>
                </tr>
            </table>
        </fieldset>
    </form>
</body>